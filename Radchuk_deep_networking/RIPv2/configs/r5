R5(config)#$ist deny_lo_1.1.1.1/32_new123 seq 10 permit 0.0.0.0/0 le 32      
R5(config)#ip pref
R5(config)#ip prefix-list PERMIT_ALL seq 5 permit 0.0.0.0/0 le 32
R5(config)#router rip
R5(config-router)#distr
R5(config-router)#distribute-list pref
R5(config-router)#distribute-list prefix PERMIT_ALL ?   
  gateway  Filtering incoming updates based on gateway
  in       Filter incoming routing updates
  out      Filter outgoing routing updates

R5(config-router)#$x PERMIT_ALL gateway deny_lo_1.1.1.1/32_new123 in         
R5(config-router)#exit
R5(config)#exit
